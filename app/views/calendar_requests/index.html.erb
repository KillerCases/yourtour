<h1>Listing calendar_requests</h1>

<table>
  <thead>
    <tr>
      <th>Tour</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calendar_requests.each do |calendar_request| %>
      <tr>
        <td><%= calendar_request.tour_id %></td>
        <td><%= calendar_request.user_id %></td>
        <td><%= link_to 'Show', calendar_request %></td>
        <td><%= link_to 'Edit', edit_calendar_request_path(calendar_request) %></td>
        <td><%= link_to 'Destroy', calendar_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calendar request', new_calendar_request_path %>
